{
	"name": "DEST",
	"version": "0.0.1",
	"private": true,
	"description": "DEST is a video player, which compiles a topic into a video playlist.",
	"main": "src/electron.cjs",
	"type": "module",
	"scripts": {
		"dev": "cross-env NODE_ENV=dev npm run dev:all",
		"dev:all": "concurrently -n=svelte,electron -c='#ff3e00',blue \"npm run dev:svelte\" \"npm run dev:electron\"",
		"dev:svelte": "svelte-kit dev",
		"dev:electron": "electron src/electron.cjs",
		"build": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron",
		"build:win": "cross-env NODE_ENV=production npm run build:svelte && npm run build:eletronwin",
		"build:lin": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electronlin",
		"build:svelte": "svelte-kit build",
		"build:electronwin": "electron-builder -w --publish never --config build.config.json",
		"build:electronlin": "electron-builder -l --publish never --config build.config.json",
		"build:electron": "electron-builder -wl --publish never --config build.config.json",
		"start": "svelte-kit start",
		"prepare": "svelte-kit sync && npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	
	"browserslist": [
		"Chrome 89"
	],
	"dependencies": {
		"@smui/chips": "^6.0.0-beta.16",
		"@smui/dialog": "^6.0.0-beta.16",
		"@smui/fab": "^6.0.0-beta.16",
		"@smui/icon-button": "^6.0.0-beta.16",
		"@smui/menu-surface": "^6.0.0-beta.16",
		"@smui/textfield": "^6.0.0-beta.16",
		"@smui/tooltip": "^6.0.0-beta.16",
		"@smui/top-app-bar": "^6.0.0-beta.16",
		"custom-electron-titlebar": "^4.1.0",
		"electron-context-menu": "^2.5.2",
		"electron-reloader": "^1.2.3",
		"electron-serve": "^1.1.0",
		"electron-window-state": "^5.0.3",
		"get-youtube-instance": "^1.0.1",
		"querystring": "^0.2.1",
		"svelte-loading-overlay": "^0.2.1",
		"youtube-iframe-player": "^1.0.1",
		"youtube-player": "^5.5.2",
		"youtube-search": "^1.1.6"
	},
	"devDependencies": {
		"@smui/button": "^6.0.0-beta.16",
		"@smui/card": "^6.0.0-beta.16",
		"@sveltejs/adapter-static": "^1.0.0-next.29",
		"@sveltejs/kit": "next",
		"@types/electron-window-state": "^2.0.34",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"concurrently": "^6.5.1",
		"cross-env": "^7.0.3",
		"dotenv": "^8.6.0",
		"electron": "^15.5.5",
		"electron-builder": "^22.14.13",
		"electron-connect": "^0.6.3",
		"electron-packager": "^15.4.0",
		"electron-updater": "^4.6.5",
		"eslint": "^7.32.0",
		"eslint-plugin-svelte": "^1.1.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.50.0",
		"smui-theme": "^6.0.0-beta.16",
		"svelte": "^3.47.0",
		"svelte-check": "^1.6.0",
		"svelte-preprocess": "^4.10.6",
		"typescript": "^4.9.4",
		"vite": "^2.9.5"
	}
}
